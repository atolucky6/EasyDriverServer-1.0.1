<UserControl
    x:Class="EasyScada.Core.DesignAnimateView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:design="clr-namespace:EasyScada.Core.Designer"
    xmlns:local="clr-namespace:EasyScada.Core"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/EasyScada.Core;component/Theme/TabControl.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid Background="#FFD5DFE5">
        <design:TabControl Style="{StaticResource Office2010SilverTabControlStyle}">
            <design:TabItem Width="80" Header="Properties" />
            <design:TabItem Width="80" Header="Animations" />
            <design:TabItem Width="80" Header="Events" />
        </design:TabControl>
    </Grid>
</UserControl>
