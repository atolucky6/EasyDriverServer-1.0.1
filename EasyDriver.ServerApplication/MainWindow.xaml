<dx:ThemedWindow
    x:Class="EasyScada.ServerApplication.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
    xmlns:dxcore="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
    xmlns:local="clr-namespace:EasyScada.ServerApplication"
    Title="Message"
    Width="1100"
    Height="700"
    Padding="0"
    Background="White"
    Icon="EasyScadaLogo.ico"
    ShowIcon="True"
    ShowInTaskbar="True"
    ShowTitle="False"
    TitleAlignment="Center">
    <dx:ThemedWindow.ToolbarItems>
        <dxb:MainMenuControl VerticalAlignment="Center" dxb:MergingProperties.AllowMerging="True" />
    </dx:ThemedWindow.ToolbarItems>
    <local:MainView />
</dx:ThemedWindow>
